<tr>
  <td class="content-block">
    <h3><%= title %></h3>
  </td>
</tr>
<tr>
  <td class="content-block">
    <table width="100%" cellpadding="0" cellspacing="0" >
      
      <% rows.map do |winner| %>
        <tr>
          <td width="75%" class="content-block">
            <%= winner[0] %>
          </td>
          <% 
            td_class = "content-block"
            negative = winner[1].starts_with?("(")
            td_class += " negative" if negative
          %>
          <td class="<%= td_class %>">
            <%= winner[1] %>
          </td>

        </tr>
      <% end %>	
    </table>
  </td>
</tr>